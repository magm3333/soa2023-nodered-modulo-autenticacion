    {
        "id": "840bcbb0383e97c9",
        "type": "function",
        "z": "a6099c015afa8015",
        "name": "Check URL & Token",
        "func": "let authtoken = msg.authtoken;\nif (!authtoken)\n    authtoken = \"nada\";\nflow.set('authtoken', authtoken);\n\nlet url = flow.get('urlBase') || msg.urlBase;\nif (!url) {\n    return [null, { payload: \"No se ha definido msg.urlBase\" }];\n}\nif (url && url.length > 5 && (url.startsWith(\"http\") || url.startsWith(\"https\"))) {\n    if (!url.endsWith(\"/\")) {\n        url += \"/\";\n    }\n    //node.warn(url)\n    flow.set('urlBase', url);\n    return [msg, null];\n}\n\nreturn [null, { payload: \"Error en definici√≥n de urlBase\" }];",
        "outputs": 2,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 240,
        "y": 80,
        "wires": [
            [
                "e09600783fdd80e6"
            ],
            []
        ],
        "outputLabels": [
            "OK",
            "Error"
        ]
    },
